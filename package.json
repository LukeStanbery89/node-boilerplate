{
    "name": "my-node-app",
    "version": "1.0.0",
    "description": "An app for auto-trading cryptocurrency",
    "author": "Luke Stanbery",
    "license": "ISC",
    "scripts": {
        "build": "npm run build:client && npm run build:server",
        "build:client": "cd client && npm run build",
        "build:server": "webpack --config webpack.server.js",
        "dev": "node scripts/start-dev.js",
        "dev:client": "cd client && npm run dev",
        "dev:server:watch": "webpack --watch --config webpack.server.js",
        "dev:server:serve": "nodemon dist/app.bundle.js",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "prepare": "husky install",
        "start": "node dist/app.bundle.js",
        "test": "npm run test:server && npm run test:client",
        "test:client": "cd client && npm run test",
        "test:server": "jest"
    },
    "dependencies": {
        "@popperjs/core": "^2.11.8",
        "@reduxjs/toolkit": "^1.9.5",
        "bootstrap": "^5.3.1",
        "express": "^4.21.2",
        "express-rate-limit": "^6.8.1",
        "node-sass": "^9.0.0",
        "react-redux": "^8.1.2",
        "react-router-dom": "^6.14.2",
        "redux": "^4.2.1",
        "redux-thunk": "^2.4.2",
        "socket.io": "^4.7.1",
        "socket.io-client": "^4.7.1",
        "webpack-dev-middleware": "^6.1.2",
        "webpack-hot-middleware": "^2.25.4"
    },
    "devDependencies": {
        "@babel/core": "^7.22.9",
        "@babel/preset-env": "^7.22.9",
        "@babel/preset-react": "^7.22.5",
        "@babel/preset-typescript": "^7.22.5",
        "@lukestanbery/meta-importer": "^1.0.1",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.3",
        "@types/node": "^20.4.6",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^5.61.0",
        "@typescript-eslint/parser": "^5.61.0",
        "autoprefixer": "^10.4.14",
        "chalk": "^5.3.0",
        "concurrently": "^8.2.0",
        "css-loader": "^6.8.1",
        "dotenv": "^16.3.1",
        "eslint": "^8.44.0",
        "eslint-plugin-react": "^7.32.2",
        "html-webpack-plugin": "^5.5.3",
        "husky": "^8.0.3",
        "jest": "^29.6.2",
        "jest-environment-jsdom": "^29.6.1",
        "jest-fetch-mock": "^3.0.3",
        "nodemon": "^3.0.1",
        "postcss-loader": "^7.3.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-refresh": "^0.14.0",
        "sass": "^1.64.2",
        "sass-loader": "^13.3.2",
        "style-loader": "^3.3.3",
        "terser-webpack-plugin": "^5.3.9",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.4.4",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.1.6",
        "webpack": "^5.94.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1",
        "webpack-node-externals": "^3.0.0"
    }
}
